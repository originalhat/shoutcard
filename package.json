{
  "name": "shoutcard",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/redux": "^3.6.0",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "object-assign": "^4.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "typescript": "^2.6.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  },
  "scripts": {
    "serve": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode=development",
    "test": "./node_modules/jest/bin/jest.js --watch",
    "test:ci": "./node_modules/jest/bin/jest.js"
  },
  "jest": {
    "testRegex": "/src/.*\\.*test.(ts|tsx)$",
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.css$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
